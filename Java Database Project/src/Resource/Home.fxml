                            <TableColumn fx:id="customerId" prefWidth="75.0" text="ID" />
                            <TableColumn fx:id="customerFirst" prefWidth="75.0" text="First Name" />
                              <TableColumn fx:id="customerLast" prefWidth="75.0" text="Last Name" />
                              <TableColumn fx:id="customerPhone" prefWidth="75.0" text="Phone" />
                              <TableColumn fx:id="customerEmail" prefWidth="120.0" text="Email" />
                          </columns>
                        </TableView>
                        <Button fx:id="insertCustomer" layoutX="31.0" layoutY="305.0" mnemonicParsing="false" text="Insert" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab fx:id="employeesTab" text="Employees">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TableView fx:id="employeeTable" layoutX="387.0" layoutY="235.0" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="200.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="100.0">
                          <columns>
                            <TableColumn fx:id="employeeId" prefWidth="75.0" text="ID" />
                            <TableColumn fx:id="password" prefWidth="75.0" text="Password" />
                              <TableColumn fx:id="employeeFirst" prefWidth="75.0" text="First Name" />
                              <TableColumn fx:id="employeeLast" prefWidth="75.0" text="Last Name" />
                              <TableColumn fx:id="employeePhone" prefWidth="75.0" text="Phone" />
                              <TableColumn fx:id="employeeEmail" prefWidth="120.0" text="Email" />
                          </columns>
                        </TableView>
                     </children></AnchorPane>
            </content>
          </Tab>
            <Tab fx:id="inventoryTab" text="Inventory">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TableView fx:id="inventoryTable" layoutX="439.0" layoutY="235.0" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="200.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="100.0">
                          <columns>
                            <TableColumn fx:id="productCode" prefWidth="100.0" text="Code" />
                            <TableColumn fx:id="category" prefWidth="75.0" text="Category" />
                              <TableColumn fx:id="name" prefWidth="130.0" text="Name" />
                              <TableColumn fx:id="price" prefWidth="75.0" text="Price" />
                              <TableColumn fx:id="stock" prefWidth="75.0" text="Amount" />
                          </columns>
                        </TableView>
                     </children></AnchorPane>
              </content>
            </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
