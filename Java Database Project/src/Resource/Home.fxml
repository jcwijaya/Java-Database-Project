        <tabs>
          <Tab fx:id="customersTab" text="Customers">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                      <Button fx:id="updateCustomer" layoutX="26.0" layoutY="366.0" mnemonicParsing="false" text="Update" />
                        <TableView fx:id="customerTable" layoutX="199.0" layoutY="-60.0" prefHeight="371.0" prefWidth="501.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="200.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="100.0">
